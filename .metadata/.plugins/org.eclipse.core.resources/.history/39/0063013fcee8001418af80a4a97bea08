package Testumgebung;

import java.awt.Color;
import java.awt.Graphics;

import javax.swing.JPanel;

public class ZeichneObjekt extends JPanel{
	int xstart, ystart, radius;
	
    public ZeichneObjekt(int xstart, int ystart, int xende, int yende){
    	this.xstart = xstart;
    	this.ystart = ystart;
    	this.radius = xende;
    }
	public void zeichnen(Graphics g){
		
    }
	
	public int getXPos(){
		return xstart;
	}
	public int getYPos(){
		return ystart;
	}
	public int getRadius(){
		return radius;
	}
	
}
